<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#ffffff"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BuildActivity">

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:id="@+id/sborkaBack"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onClickBack"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@android:drawable/ic_menu_revert" />

                <ImageView
                    android:id="@+id/trashbutton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.97"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@android:drawable/ic_menu_delete" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="22dp"
                    android:layout_marginBottom="10dp"
                    android:background="#FFFFFF"
                    android:text="Сборка"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/trashbutton"
                    app:layout_constraintHorizontal_bias="0.4"
                    app:layout_constraintStart_toEndOf="@+id/sborkaBack"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/motherTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Материнская плата"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/motherContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/motherIm"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/cpuTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:text="Процессор"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/cpuContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/cpuIm"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/CPUcoollerTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Кулер для процессора"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/CPUcoollerContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/CPUcoollerIm"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/gpuTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Видеокарта"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/gpuContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/gpuIm"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/ramTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Оперативная память"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/ramContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/ramIm"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/hddTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Жёсткие диски"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/hddContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/hddIm"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/ssdTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=" SSD накопитель"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/ssdContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/ssdIm"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/m2TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="M.2 накопители"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/m2Container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/m2Im"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/bpTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Блок питания"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/bpContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/bpIm"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/bodyTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Корпус"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/bodyContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/bodyIm"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/coollersTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Вентиляторы"
                android:textColor="@android:color/black"
                android:typeface="serif" />

            <FrameLayout
                android:id="@+id/coollersContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px">

                <ImageButton
                    android:id="@+id/coollersIm"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="93dp"
                    android:layout_gravity="bottom|left"
                    app:srcCompat="@drawable/sborka221" />
            </FrameLayout>

            <TextView
                android:id="@+id/priceSborka"
                android:layout_width="168dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|right"
                android:textColor="#000000" />

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>